<div class="container">
  <%= simple_form_for(@service, url: enterprise_service_path) do |f| %>
    <%= f.input :enterprise_id, as: :integer, input_html: { value: @enterprise_id }, :as => :hidden %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
</div>
